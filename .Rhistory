get_gradient_fit <- function(dta){
num_cases <- dta %>%
filter(date == max(date)) %>%
pull(n)
model <- lm(log(n) ~ date, data = dta)
slope <- coefficients(model)[["date"]]
rsq <- summary(model)$r.squared
return(tibble(
num_cases, slope = slope, r_squared = rsq
))
}
